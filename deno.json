{
  "imports": {
    "hono": "jsr:@hono/hono@^4.6.9",
    "@/types/": "./src/types/",
    "@/services/": "./src/services/",
    "@/utils/": "./src/utils/",
    "@/constants/": "./src/constants/",
    "@/routes/": "./src/routes/",
    "@/controllers/": "./src/controllers/"
  },
  "tasks": {
    "dev": "deno run -A --watch ./src/main.ts",
    "start": "deno run --allow-net main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
